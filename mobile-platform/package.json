{
  "name": "@noice-com/mobile-platform",
  "version": "1.0.3",
  "private": true,
  "prettier": "@noice-com/eslint-config/prettier.config.js",
  "scripts": {
    "android": "yarn android:stg",
    "android:dev": "ENVFILE=./envs/dev.env react-native run-android",
    "android:stg": "ENVFILE=./envs/stg.env react-native run-android",
    "android:prd": "ENVFILE=./envs/prd.env react-native run-android",
    "android:log": "react-native log-android",
    "ios": "yarn ios:stg",
    "ios:dev": "ENVFILE=./envs/dev.env react-native run-ios --scheme=\"Noice (Dev)\"",
    "ios:stg": "ENVFILE=./envs/stg.env react-native run-ios --scheme=\"Noice (Stg) (Local)\"",
    "ios:prd": "ENVFILE=./envs/prd.env react-native run-ios --scheme=\"Noice (Prd) (Local)\"",
    "clean": "watchman shutdown-server && react-native start --reset-cache",
    "lint": "eslint .",
    "test": "jest",
    "clean:ios": "rm -rf ./node_modules && rm -rf ../../node_modules && yarn && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install --repo-update",
    "assets:link": "react-native-asset",
    "dependencies:check": "rnx-align-deps",
    "dependencies:fix": "rnx-align-deps --fix",
    "graphql:codegen:build": "graphql-codegen --config codegen.yml",
    "graphql:codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "postinstall": "patch-package",
    "check-types": "tsc --noEmit --project tsconfig.types.json",
    "navigate:tobias": "node universal-links -- u/toboas",
    "navigate": "node universal-links",
    "test:mobile": "jest --testPathPattern=Noice mobile",
    "notification": "xcrun simctl push booted com.noice.MobilePlatform.stg ./ios/stg-notification.apns",
    "notification:prd": "xcrun simctl push booted com.noice.MobilePlatform ./ios/prd-notification.apns"
  },

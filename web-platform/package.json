{
  "name": "@noice-com/web-platform",
  "packageManager": "yarn@4.1.0",
  "version": "1.0.0",
  "author": "Noice",
  "description": "Noice Web Client",
  "repository": "git@github.com:noice-com/platform.git",
  "license": "NONE",
  "private": true,
  "prettier": "@noice-com/eslint-config/prettier.config.js",
  "sideEffects": [
    "./src/globals/window-noice-helpers.ts"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.4.0"
  },
  "workspaces": [
    "../../lib/ts/common-ui",
    "../../lib/ts/common-react-core",
    "../../lib/ts/chat-react-web",
    "../../lib/ts/chat-react-core",
    "../../lib/ts/social",
    "../../lib/ts/social-react-core",
    "../../lib/ts/stream",
    "../../lib/ts/card-game"
  ],
  "scripts": {
    "build": "yarn run codegen:build-all && NODE_ENV=production vite build --mode prd && esbuild server.ts --bundle --outfile=server_pack.js --platform=node --external:fsevents --external:esbuild --external:vite --external:rollup",
    "build-dev": "yarn run codegen:build-all && vite build --mode dev",
    "build-stg": "yarn run codegen:build-all && vite build --mode stg",
    "build-all-stg": "yarn run codegen:build-all && (concurrently -n \"build-client,build-storybook\" \"vite build --mode stg\" \"yarn build-storybook\")",
    "lint": "concurrently -n \"check-types,eslint,stylelint,circular-deps\" \"yarn check-types\" \"yarn lint:ts\" \"yarn lint:css\" \"yarn check-no-circular-deps\"",
    "lint:fix": "concurrently -n \"fix-eslint,fix-css\" \"yarn lint:ts:fix\" \"yarn lint:css:fix\"",
    "lint:css": "concurrently -n \"stylelint,prettier\" \"stylelint './src/**/*.css' --color\" \"prettier --check  './src/**/*.css'\"",
    "lint:css:fix": "stylelint './src/**/*.css' --fix && prettier --write './src/**/*.css'",
    "lint:ts": "TIMING=1 eslint ./src --color",
    "lint:ts:fix": "eslint ./src --fix",
    "format": "yarn prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "yarn prettier --list-different \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "test": "jest --passWithNoTests",
    "test-scripts": "jest --config=jest.scripts.config.js ./scripts",
    "dev-serve-dev": "NODE_ENV=development ts-node -r dotenv/config server.ts dotenv_config_path=./.env/.env.dev",
    "dev-serve-local": "NODE_ENV=development ts-node -r dotenv/config server.ts dotenv_config_path=./.env/.env",
    "dev-serve-stg": "NODE_ENV=development ts-node -r dotenv/config server.ts dotenv_config_path=./.env/.env.stg",
    "dev-serve-prd": "NODE_ENV=development ts-node -r dotenv/config server.ts dotenv_config_path=./.env/.env.prd",
    "codegen:build-all": "yarn workspaces foreach -vpRi -j unlimited --include @noice-com/common-ui --include @noice-com/common-react-core --include @noice-com/stream --include @noice-com/web-platform --include @noice-com/chat-react-web --include @noice-com/chat-react-core --include @noice-com/card-game --include @noice-com/social --include @noice-com/social-react-core run graphql:codegen:build",
    "codegen:watch-all": "yarn workspaces foreach -vpRi -j unlimited --include @noice-com/common-ui --include @noice-com/common-react-core --include @noice-com/stream --include @noice-com/web-platform --include @noice-com/chat-react-web --include @noice-com/chat-react-core --include @noice-com/card-game --include @noice-com/social --include @noice-com/social-react-core run graphql:codegen:watch",
    "check-types": "tsc --build tsconfig.types.json",
    "check-no-circular-deps": "dpdm -T --no-warning --no-tree --exit-code circular:1 ./src/index.tsx",
    "serve-optimized": "node server_pack.js",
    "watch-serve-local": "yarn codegen:watch-all & yarn dev-serve-local",
    "watch-serve-dev": "yarn codegen:watch-all & yarn dev-serve-dev",
    "watch-serve-stg": "yarn codegen:watch-all & yarn dev-serve-stg",
    "watch-serve-prd": "yarn codegen:watch-all & yarn dev-serve-prd",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "STORYBOOK_ENABLE_CRASH_REPORTS=0 storybook build",
    "graphql:codegen:build": "graphql-codegen --config codegen.yml",
    "graphql:codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "generate-component": "node ./scripts/generate-component.js",
    "oss-disclaimer": "yarn licenses generate-disclaimer --prod -s > DISCLAIMER.md",
    "visualize": "vite-bundle-visualizer -c vite.config.mjs",
    "imported-by": "yarn visualize -t raw-data -o ${TMPDIR}wpimportstats.json  && node ./scripts/import_analyzer.js ${TMPDIR}wpimportstats.json"
  },
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.6.1",
    "@floating-ui/dom": "^1.5.2",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@noice-com/esbuild": "workspace:*",
    "@noice-com/esbuild-plugins": "workspace:*",
    "@noice-com/eslint-config": "workspace:*",
    "@noice-com/eslint-config-react": "workspace:*",
    "@noice-com/ftue-config": "workspace:*",
    "@noice-com/postcss": "workspace:*",
    "@noice-com/vite-plugins": "workspace:*",
    "@parcel/watcher": "^2.3.0",
    "@sentry/vite-plugin": "^2.10.1",
    "@storybook/addon-a11y": "^7.0.8",
    "@storybook/addon-actions": "^7.0.8",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@types/express": "^4.17.17",
    "@types/howler": "^2.2.8",
    "@types/jest": "^29.5.4",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.merge": "^4.6.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/three": "^0.168.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "dpdm": "^3.14.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.48.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "storybook": "^7.0.8",
    "storybook-addon-apollo-client": "^4.1.4",
    "stylelint": "^15.10.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-glsl": "^1.1.2",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@adyen/adyen-web": "^5.51.0",
    "@apollo/client": "^3.7.17",
    "@braintree/browser-detection": "^1.17.2",
    "@floating-ui/react": "^0.26.0",
    "@noice-com/apollo-client-utils": "workspace:*",
    "@noice-com/assets-core": "workspace:*",
    "@noice-com/avatar-builder": "workspace:*",
    "@noice-com/card-game": "workspace:*",
    "@noice-com/chat-react-web": "workspace:*",
    "@noice-com/common-react-core": "workspace:*",
    "@noice-com/common-ui": "workspace:*",
    "@noice-com/platform-client": "workspace:*",
    "@noice-com/schemas": "workspace:*",
    "@noice-com/server": "workspace:*",
    "@noice-com/social": "workspace:*",
    "@noice-com/social-react-core": "workspace:*",
    "@noice-com/stream": "workspace:*",
    "@noice-com/utils": "workspace:*",
    "@noice-com/web-renderer": "workspace:*",
    "@noice-com/webview-bridge": "workspace:*",
    "@reach/dialog": "^0.18.0",
    "@reach/portal": "^0.18.0",
    "@restart/hooks": "^0.4.11",
    "@sentry/integrations": "^7.81.1",
    "@sentry/react": "^7.81.1",
    "@webview-bridge/web": "^1.4.1",
    "crypto-hash": "^3.0.0",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "firebase": "^10.12.2",
    "google-protobuf": "^3.21.2",
    "graphql": "^16.8.0",
    "howler": "^2.2.3",
    "jwt-decode": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "markdown-to-jsx": "^7.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-on": "^3.9.1",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.11.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "three": "^0.168.0",
    "use-media": "^1.4.0",
    "vite-bundle-visualizer": "^1.1.0"
  }
}
